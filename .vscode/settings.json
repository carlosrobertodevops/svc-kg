{
	"editor.lineHeight": 23,
	"markdown.preview.openMarkdownLinks": "inEditor",
	  // Garante que .md/.html/.svg/.pdf abram no editor padrão (não no preview/webview)
  "workbench.editorAssociations": [
    { "viewType": "default", "filenamePattern": "*.md" },
    { "viewType": "default", "filenamePattern": "*.html" },
    { "viewType": "default", "filenamePattern": "*.svg" },
    { "viewType": "default", "filenamePattern": "*.pdf" }
  ]
}